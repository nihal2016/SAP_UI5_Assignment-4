{
	"_version": "1.12.0",
	"sap.app": {
		"id": "com.yash.assignment-4",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.mobile": {
		"_version": "1.1.0",
		"definingRequests": {},
		"stores": [
			{
				"name": "assignment-4"
			}
		]
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.yash.assignment-4.view.View1",
			"type": "XML",
			"async": true,
			"id": "View1"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.yash.assignment-4.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.yash.assignment-4.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "RouteView1",
					"pattern": "RouteView1",
					"target": [
						"TargetView1"
					]
				},
				{
					"name": "RouteView2",
					"pattern": "RouteView2",
					"target": [
						"TargetView2"
					]
				},
				{
					"name": "RouteView3",
					"pattern": "RouteView3",
					"target": [
						"TargetView3"
					]
				},
				{
					"name": "RouteView4",
					"pattern": "RouteView4",
					"target": [
						"TargetView4"
					]
				},
				{
					"name": "RouteView5",
					"pattern": "RouteView5",
					"target": [
						"TargetView5"
					]
				}
			],
			"targets": {
				"TargetView1": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "View1"
				},
				"TargetView2": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "View2"
				},
				"TargetView3": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "View3"
				},
				"TargetView4": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "View4"
				},
				"TargetView5": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "View5"
				},
				"view7": {
					"viewType": "XML",
					"viewName": "view7"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}